{
  "name": "freemix",
  "version": "0.0.0",
  "private": true,
  "description": "A powerful cross-platform desktop application built with Electron and Vue.js",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:build:all": "npm run build && electron-builder -mwl",
    "electron:build:mac": "npm run build && ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ electron-builder",
    "electron:build:win-mirror": "npm run build && ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ ELECTRON_BUILDER_BINARIES_MIRROR=https://npmmirror.com/mirrors/electron-builder-binaries/ electron-builder --win",
    "electron:build:win": "npm run build && electron-builder --win --config.win.sign=null --config.win.signAndEditExecutable=false",
    "electron:build:mac": "npm run build && electron-builder --mac --config.mac.identity=null",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@logicflow/core": "1.2.27",
    "@logicflow/extension": "1.2.27",
    "@types/marked": "^5.0.2",
    "@vue-flow/additional-components": "^1.3.3",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.46.0",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.10.6",
    "lucide-vue-next": "^0.525.0",
    "marked": "^16.3.0",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "vant": "^4.9.21",
    "vue": "^3.5.17",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vicons/ionicons5": "^0.13.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "naive-ui": "^2.42.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10",
    "wait-on": "^9.0.1"
  }
}
